Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Egaussx", "[", 
    RowBox[{"Ek_", ",", "d_", ",", "\[Tau]0_", ",", "t_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"-", 
     FractionBox[
      RowBox[{"N0", "*", "QE", "*", 
       RowBox[{"gamma", "[", "Ek", "]"}]}], 
      RowBox[{"4", "*", "\[Pi]", "*", "EPSILON0", "*", 
       SuperscriptBox["d", "2"]}]]}], "*", 
    FractionBox["1", 
     RowBox[{
      SqrtBox["\[Pi]"], "\[Tau]0", "*", 
      RowBox[{"Erf", "[", "2", "]"}]}]], "*", 
    RowBox[{"NIntegrate", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Exp", "[", 
        RowBox[{"-", 
         FractionBox[
          SuperscriptBox["x", "2"], 
          SuperscriptBox["\[Tau]0", "2"]]}], "]"}], "*", 
       FractionBox["1", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"1", "+", 
           RowBox[{
            SuperscriptBox[
             RowBox[{"beta", "[", "Ek", "]"}], "2"], "*", 
            SuperscriptBox[
             RowBox[{"gamma", "[", "Ek", "]"}], "2"], "*", 
            SuperscriptBox["c", "2"], "*", 
            RowBox[{
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{"x", "+", "t"}], ")"}], "2"], "/", 
             SuperscriptBox["d", "2"]}]}]}], ")"}], 
         RowBox[{"3", "/", "2"}]]]}], ",", 
      RowBox[{"{", 
       RowBox[{"x", ",", 
        RowBox[{
         RowBox[{"-", "2"}], "\[Tau]0"}], ",", 
        RowBox[{"2", "\[Tau]0"}]}], "}"}]}], "]"}]}]}], ";"}]], "Input",
